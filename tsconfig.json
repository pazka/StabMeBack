  "compilerOptions": {
    "module": "CommonJS",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "lib": ["es6"]
  },
  "files" : [
    "index.ts",
  ],
  "include": ["*.ts","./**/*.ts"],
